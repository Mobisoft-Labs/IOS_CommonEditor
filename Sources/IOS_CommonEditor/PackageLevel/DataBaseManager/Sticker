+//
+//  OverlayPannelVM.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 30/11/23.
+//
+
+import Foundation
+
+
+class OverlayPannelVM: overlayDelegate {
+    func onChangeOverlay(overlay: String) {
+        print(overlay)
+    }
+    
+    @Published var currentOverlayOpacity = 0
+    var currentOverlay:String?
+    init(){
+     
+    }
+    
+    lazy var overlayCVVM:OverlayCVVM = {
+        return OverlayCVVM(delegate: self)
+    }()
+    
+    func didChangeOpacity(value:Float){
+        printLog(value)
+    }
+    
+    func setModel(){
+        if let model = DBManager.shared.getBaseModelFromDB(modelId: 1186){
+            currentOverlayOpacity = model.overlayOpacity
+            if let overlayModel = DBManager.shared.getImage(imageId: model.overlayDataId),overlayModel.imageType == "OVERLAY"{
+                currentOverlay = overlayModel.resID
+            }else{
+                currentOverlay = "none"
+            }
+        }
+    }
+}
