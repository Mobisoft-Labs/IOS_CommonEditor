diff --git a/VideoInvitation/StickerPicker/StickerCView/StickerCVVM.swift b/VideoInvitation/StickerPicker/StickerCView/StickerCVVM.swift
new file mode 100644
index 0000000..cd997cd
--- /dev/null
+++ b/VideoInvitation/StickerPicker/StickerCView/StickerCVVM.swift
@@ -0,0 +1,42 @@
+//
+//  StickerCVVM.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 08/11/23.
+//
+
+import Foundation
+
+protocol StickerCVDelegate:AnyObject{
+    
+}
+
+class StickerCVVM{
+    private var currentModelID = -1
+    private var currentModelArrayCategory = " "
+    
+    @Published var onSelectModelIndex:IndexPath = IndexPath(item: -1, section: 0)
+    @Published var onStickerModelArrayLoaded : Bool = false
+    var strickerArray = [StickerAsset]()
+    
+    weak var stickerCVDelegate:StickerCVDelegate?
+
+    
+    init(stickerCVDelegate:StickerCVDelegate){
+        self.stickerCVDelegate = stickerCVDelegate
+    }
+    
+    func getStickerModelArrayCount()->Int{
+        return strickerArray.count
+    }
+    
+    func getStickerModelFor(index:Int)->StickerAsset{
+        return strickerArray[index]
+    }
+    
+    func setStickerCV(stickerArray:[StickerAsset]){
+        self.strickerArray = stickerArray
+        onStickerModelArrayLoaded = true
+    }
+    
+}
