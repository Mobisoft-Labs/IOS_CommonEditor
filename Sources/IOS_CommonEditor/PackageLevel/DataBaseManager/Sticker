diff --git a/VideoInvitation/Music_manager/Fav Music/IsFavMusicVM.swift b/VideoInvitation/Music_manager/Fav Music/IsFavMusicVM.swift
new file mode 100644
index 0000000..866aca7
--- /dev/null
+++ b/VideoInvitation/Music_manager/Fav Music/IsFavMusicVM.swift	
@@ -0,0 +1,42 @@
+//
+//  IsFavMusicVM.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 21/11/23.
+//
+
+import Foundation
+
+protocol ISFavMusicDelegate:AnyObject{
+    
+}
+
+class IsFavMusicVM{
+    
+   @Published var isMusicArrayFetched = false
+     var musicArray = [MusicModel]()
+    weak var favMusicDelegate:ISFavMusicDelegate?
+    init(delegate:ISFavMusicDelegate){
+        self.favMusicDelegate = delegate
+     
+    }
+    
+    func fetchIsFavMusicArray(){
+        musicArray = DBManager.shared.fetchFavoriteMusicModels()
+        isMusicArrayFetched = true
+    }
+    
+    func getCountOfMusicArray()->Int{
+        return musicArray.count
+    }
+    
+    
+    func getMusicModelFor(index:Int)->MusicModel{
+        return musicArray[index]
+    }
+    
+    func setModel(){
+        fetchIsFavMusicArray()
+    }
+    
+}
diff --git a/VideoInvitation/Music_manager/MusicDBManager.swift b/VideoInvitation/Music_manager/MusicDBManager.swift
