+//
+//  Created by HKBeast on 27/11/23.
+//
+
+import Foundation
+
+class PageControllerVM: PageControllerCVVMDelegate{
+    
+    
+    init(){
+        
+    }
+    
+    lazy var pageControllerCVVM = {
+       let viewModel = PageControllerCVVM(delegate: self)
+        return viewModel
+    }()
+    
+    func onClickAddPage(){
+       _ = pageControllerCVVM.addPage()
+    }
+    
+    
+    func onClickDuplicatePage(for id:Int){
+        pageControllerCVVM.addDuplicatePage(for: id)
+    }
+    
+    func onClickDeletePage(for id:Int){
+        pageControllerCVVM.deletePageFor(id: id)
+    }
+}
