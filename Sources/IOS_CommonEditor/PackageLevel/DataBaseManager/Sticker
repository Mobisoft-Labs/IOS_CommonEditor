diff --git a/VideoInvitation/StickerPicker/StickerTab/StickerTabVM.swift b/VideoInvitation/StickerPicker/StickerTab/StickerTabVM.swift
new file mode 100644
index 0000000..c3a3786
--- /dev/null
+++ b/VideoInvitation/StickerPicker/StickerTab/StickerTabVM.swift
@@ -0,0 +1,53 @@
+//
+//  StickerTabVM.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 08/11/23.
+//
+
+import Foundation
+
+protocol StickerTabDelegate:AnyObject{
+    func onChangeCategory(category:String)
+}
+
+
+class StickerTabVM{
+    @Published var currentCategory:String = " "
+     
+     @Published var onCategoryListLoaded : Bool = false
+     var categoryList = [String]() {
+         didSet {
+             onCategoryListLoaded = true
+         }
+     }
+     
+
+     weak var stickerTabDelegate: StickerTabDelegate?
+
+     
+     init(stickerTabDelegate:StickerTabDelegate){
+         self.stickerTabDelegate = stickerTabDelegate
+         
+     }
+     
+     
+     func getFontCategories()->[String]{
+         return categoryList
+     }
+     
+     func getFontCategoriesCount()->Int{
+         return categoryList.count
+     }
+     
+  
+     func setFontTabVM(categories:[String]){
+         categoryList = categories
+         onCategoryListLoaded = true
+     }
+    
+    func didSelectCategory(at:Int){
+        let category = categoryList[at]
+        stickerTabDelegate?.onChangeCategory(category: category)
+    }
+}
