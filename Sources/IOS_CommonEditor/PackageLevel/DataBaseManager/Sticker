+//
+//  StickerDB.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 08/11/23.
+//
+
+import Foundation
+
+
+extension DBManager{
+    
+    
+    func createStickerAssetTableIFNeeded()->Bool{
+        var created = false
+        if FileManager.default.fileExists(atPath: db_local_path) {
+            database = FMDatabase(path: db_local_path)
+            
+            printLog("db local path:- ",db_local_path)
+            //create animation table
+    
+            
+            
+    let query =
+             """
+              CREATE TABLE IF NOT EXISTS \(TABLE_STIKCER_ASSET)  (
+        \(Sticker_Id) INTEGER PRIMARY KEY,
+        \(Sticker_Category) TEXT,
+        \(Sticker_DisplaySequence) INTEGER,
+        \(Sticker_ResourceType) TEXT,
+        \(Sticker_ImageName) TEXT,
+        \(Sticker_ImageLocalPath) TEXT,
+        \(Sticker_ImageServerPath) TEXT,
+        \(Sticker_ImageRelativePath) TEXT,
+        \(Sticker_ImageFileExtension) TEXT,
+        \(Sticker_ImageThumbServerPath) TEXT,
+        \(Sticker_ImageThumbRelativePath) TEXT,
+        \(Sticker_ImageThumbFileExtension) TEXT,
+        \(Sticker_ImageWidth) INTEGER,
+        \(Sticker_ImageHeight) INTEGER,
+        \(Sticker_ThumbWidth) INTEGER,
+        \(Sticker_ThumbHeight) INTEGER,
+        \(Sticker_Tags) TEXT,
+        \(Sticker_IsFree) INTEGER,
+        \(Sticker_IsLocal) INTEGER,
+        \(Sticker_IsMarked) INTEGER,
+        \(Sticker_IsEncrypted) INTEGER,
+        \(Sticker_FutureField1) TEXT,
+        \(Sticker_FutureField2) TEXT,
+        \(Sticker_FutureField3) TEXT,
+        \(Sticker_FutureField4) TEXT
+    );
+    """
+    
+            
+            do {
+                //        for query in querys{
+                try updateQuery(query, values: nil)
+                //        }
+                
+                printLog("true")
+                created = true
+            }
+            catch {  printLog("Could not create table.")
+                printLog(error.localizedDescription)
+                
+            }
+        }
+        
+        return created
+    }
+
+    func insertStickerAssetTable(model: StickerAsset) -> Int {
+        var insertedRowId: Int = -1
+        
+        
+        // Build the SQL query as a long string with values
+        let query = """
+        INSERT INTO \(TABLE_STIKCER_ASSET)  (
+            \(Sticker_Category),
+            \(Sticker_DisplaySequence),
+            \(Sticker_ResourceType),
+            \(Sticker_ImageName),
+            \(Sticker_ImageLocalPath),
+            \(Sticker_ImageServerPath),
+            \(Sticker_ImageRelativePath),
+            \(Sticker_ImageFileExtension),
+            \(Sticker_ImageThumbServerPath),
+            \(Sticker_ImageThumbRelativePath),
+            \(Sticker_ImageThumbFileExtension),
+            \(Sticker_ImageWidth),
+            \(Sticker_ImageHeight),
+            \(Sticker_ThumbWidth),
+            \(Sticker_ThumbHeight),
+            \(Sticker_Tags),
+            \(Sticker_IsFree),
+            \(Sticker_IsLocal),
+            \(Sticker_IsMarked),
+            \(Sticker_IsEncrypted),
+            \(Sticker_FutureField1),
+            \(Sticker_FutureField2),
+            \(Sticker_FutureField3),
+            \(Sticker_FutureField4)
+        ) VALUES (
+            '\(model.category)',
+            \(model.displaySequence),
+            '\(model.resourceType)',
+            '\(model.imageName)',
+            '\(model.imageLocalPath)',
+            '\(model.imageServerPath)',
+            '\(model.imageRelativePath)',
+            '\(model.imageFileExtension)',
+            '\(model.imageThumbServerPath)',
+            '\(model.imageThumbRelativePath)',
+            '\(model.imageThumbFileExtension)',
+            \(model.imageWidth),
+            \(model.imageHeight),
+            \(model.thumbWidth),
+            \(model.thumbHeight),
+            '\(model.tags)',
+            \(model.isFree ? 1 : 0),
+            \(model.isLocal ? 1 : 0),
+            \(model.isMarked ? 1 : 0),
+            \(model.isEncrypted ? 1 : 0),
+            '\(model.futureField1)',
+            '\(model.futureField2)',
+            '\(model.futureField3)',
+            '\(model.futureField4)'
+        )
+        """
+
+        insertedRowId = insertNewEntry(query: query)
+
+        return Int(insertedRowId)
+    }
+
+    func fetchStickerModel(category: String) -> [StickerAsset] {
+        var stickers: [StickerAsset] = []
+
+            let query = "SELECT * FROM \(TABLE_STIKCER_ASSET) WHERE \(Sticker_Category) = '\(category)'"
+            print(query)
+            guard let resultSet = try? runQuery(query, values: []) else {
+                return []
+            }
+            
+            while resultSet.next() {
+                var sticker = StickerAsset()
+                sticker.id = Int(resultSet.int(forColumn: Font_ID))
+                sticker.category = resultSet.string(forColumn: Sticker_Category) ?? ""
+                sticker.imageName = resultSet.string(forColumn: Sticker_ImageName) ?? ""
+//                font.fontFamily = resultSet.string(forColumn: Font_fontFamily) ?? ""
+//                font.fontRealName = resultSet.string(forColumn: Font_fontRealName) ?? ""
+//                font.fontCategory = resultSet.string(forColumn: Font_fontCategory) ?? ""
+//                font.fontSource = resultSet.string(forColumn: Font_fontSource) ?? ""
+//                font.fontLocalPath = resultSet.string(forColumn: Font_fontLocalPath) ?? ""
+//                font.fontServerPath = resultSet.string(forColumn: Font_fontServerPath) ?? ""
+//                font.isPremium = Int(resultSet.int(forColumn: Font_isPremium)) == 1
+//                font.isTrending = Int(resultSet.int(forColumn: Font_isTrending)) == 1
+//                font.isFavourite = Int(resultSet.int(forColumn: Font_isFavourite)) == 1
+              
+                stickers.append(sticker)
+            }
+        
+        
+        return stickers
+    }
+    
+}
