+    func onChangeCategory(category: String) {
+        
+    }
+    
+  
+    
+    lazy var stickerCVVM:StickerCVVM = {
+        let vm = StickerCVVM(stickerCVDelegate : self)
+        return vm
+      }()
+    
+    lazy var stickerTabVM:StickerTabVM = {
+        return StickerTabVM(stickerTabDelegate : self)
+    }()
+    
+    
+    
+    weak var fontPannelDelegate:FontPanneDelegate?
+    var dataSource:FontHandlerInterface
+    
+    init(delegate:FontPanneDelegate,dataSource:FontHandlerInterface){
+        self.dataSource = dataSource
+        fontPannelDelegate = delegate
+    }
+    
