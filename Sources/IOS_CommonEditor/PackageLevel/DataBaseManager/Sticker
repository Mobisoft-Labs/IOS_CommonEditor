+++ b/VideoInvitation/FontManager/FontPannel/FontPannelVM.swift
@@ -0,0 +1,51 @@
+//
+//  FontPannelVM.swift
+//  FontManager
+//
+//  Created by HKBeast on 06/11/23.
+//
+
+import Foundation
+
+protocol FontPanneDelegate:AnyObject{
+    func onChangeFont(fontModel:FontDM)
+}
+
+class FontPannelVM: FontCollectionViewDelegate, FontTAbDelegate{
+    func didFontCategorySelected(categoryName: String) {
+        if categoryName == "Apple"{
+            fontCVVM.setFontCV(fontArray: dataSource.getAppleFontListIfNeeded())
+        }
+        else if categoryName == "Server"{
+            fontCVVM.setFontCV(fontArray: dataSource.getServerListIfNeeded())
+        }else{
+            fontCVVM.setFontCV(fontArray: dataSource.getUserFontsIfNeeded())
+        }
+    }
+    
+    
+    lazy var fontCVVM:FontCVVM = {
+        let vm = FontCVVM(ratioCVDelegate: self)
+        return vm
+      }()
+    
+    lazy var fontTabVM:FontTabVM = {
+        return FontTabVM(fontTabDelegate: self)
+    }()
+    
+    weak var fontPannelDelegate:FontPanneDelegate?
+    var dataSource:FontHandlerInterface
+    
+    init(delegate:FontPanneDelegate,dataSource:FontHandlerInterface){
+        self.dataSource = dataSource
+        fontPannelDelegate = delegate
+    }
+    
+    func setViewModel(){
+        fontTabVM.setFontTabVM(categories: ["Apple","Server","User"])
+        fontCVVM.setFontCV(fontArray: dataSource.getServerListIfNeeded())
+        
+    }
+    
+    
+}
diff --git a/VideoInvitation/FontManager/FontPannel/RatioCollectionView/FontCVCell.swift b/VideoInvitation/FontManager/FontPannel/RatioCollectionView/FontCVCell.swift
new file mode 100644
index 0000000..9d9748f
--- /dev/null
+++ b/VideoInvitation/FontManager/FontPannel/RatioCollectionView/FontCVCell.swift
@@ -0,0 +1,63 @@
+//
+//  RatioCVCell.swift
+//  RatioManager
+//
+//  Created by HKBeast on 30/10/23.
+//
+
+import UIKit
+
+class FontCVCell: UICollectionViewCell {
+
+   @IBOutlet weak var centeredView:UIView!
+    @IBOutlet weak var widthConstraint: NSLayoutConstraint!
+    @IBOutlet weak var heightConstraint: NSLayoutConstraint!
+    @IBOutlet weak var label:UILabel!
+    override func awakeFromNib() {
+        super.awakeFromNib()
+        // Initialization code
+        self.backgroundColor = .clear
+
+    
+    }
+
+    func config(model:FontModel,indexPath:IndexPath){
+        if let customFontURL = Bundle.main.url(forResource: model.fontName, withExtension: "ttf"),
+            let fontDataProvider = CGDataProvider(url: customFontURL as CFURL),
+            let font = CGFont(fontDataProvider) {
+            var error: Unmanaged<CFError>?
+            if CTFontManagerRegisterGraphicsFont(font, &error) {
+                print("Custom font registered successfully.")
+            } else if let error = error?.takeRetainedValue() {
+                print("Failed to register custom font: \(error)")
+            }
+        }
+//        let customFont = UIFont(name: model.fontName, size: 16.0)
+        
+        label.font = UIFont(name: getRealFont(nameOfFont: model.fontName), size: 16.0)
+        label.text  = model.fontName
+        
+        
+        if self.isSelected{
+            selectedCell()
+        }
+        else{
+            deSelectedCell()
+        }
+        
+    
+        
+    }
+    
+    func selectedCell(){
+        self.isSelected = true
+//        self.borderColor = .blue
+    }
+    
+    func deSelectedCell(){
+        self.isSelected = false
+//        self.borderColor = .white
+    }
+    
+   
+}
