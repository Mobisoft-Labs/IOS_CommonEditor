diff --git a/VideoInvitation/Ratio_Manager/Helper/AppDataSource.swift b/VideoInvitation/Ratio_Manager/Helper/AppDataSource.swift
new file mode 100644
index 0000000..6e9df60
--- /dev/null
+++ b/VideoInvitation/Ratio_Manager/Helper/AppDataSource.swift
@@ -0,0 +1,132 @@
+//
+//  AppDataSource.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 01/11/23.
+//
+
+import Foundation
+import UIKit
+
+
+class AppDataSource:FontHandlerInterface{
+
+    
+    private var appleFonts = [FontModel]()
+    private var serverFonts = [FontModel]()
+    private var userFonts = [FontModel]()
+    
+      static var shared = AppDataSource()
+      private var _ratioCategories = [String]()
+       var appFileManage = AppFileManager.shared
+
+       var ratioCategories : [String] {
+
+           get {
+
+               if _ratioCategories.isEmpty {
+                   _ratioCategories = DBManager.shared.getRatioCategories()
+               }
+               return _ratioCategories
+
+           }
+       }
+
+    
+    
+    var arrayOfAppleFont:[FontModel]{
+        get{
+          if self.appleFonts.count == 0{
+                appleFonts = getAppleFontListIfNeeded()
+            }
+            return appleFonts
+        }
+    }
+    var arrayOfServerFont:[FontModel]{
+        get{
+          if self.serverFonts.count == 0{
+                serverFonts = getServerListIfNeeded()
+            }
+            return serverFonts
+        }
+    }
+    
+    var arrayOfUserFont:[FontModel]{
+        get{
+          if self.userFonts.count == 0{
+                userFonts = getUserFontsIfNeeded()
+            }
+            return userFonts
+        }
+    }
+    
+    
+    internal  func getAppleFontListIfNeeded() -> [FontModel] {
+        var arrayOfFont = [FontModel]()
+       
+        for familyName:String in UIFont.familyNames {
+             for fontName:String in UIFont.fontNames(forFamilyName: familyName) {
+         
+                 let model = FontModel(fontName: fontName, family: familyName, localPath: "")
+               
+                arrayOfFont.append(model)
+             }
+         }
+        return arrayOfFont
+    }
+    
+    
+    internal func getServerListIfNeeded() -> [FontModel] {
+         let arrayOfFont = DBManager.shared.fetchFontModel(fontSource: FontSource.Server.rawValue)
+         return arrayOfFont
+     }
+     
+     internal  func getUserFontsIfNeeded() -> [FontModel] {
+         let arrayOfFont = DBManager.shared.fetchFontModel(fontSource: FontSource.User.rawValue)
+         return arrayOfFont
+     }
+     
+    
+    
+    func registerServerFont(arrayOfFont:[FontModel]){
+        for fontModel in arrayOfFont {
+            registerCustomFont(fontName: fontModel.fontName, fileExtension: ".ttf", folder: appFileManage.serverFont!)
+        }
+    }
+
+  
+    private func registerCustomFont(fontName: String, fileExtension: String, folder:Folder) {
+       if let customFontURL = Bundle.main.url(forResource: fontName, withExtension: fileExtension) {
+           let fileManager = FileManager.default
+     
+
+           do {
+               // Copy the font file to the document directory
+               let data = NSData(contentsOf: customFontURL)
+               var file = File(name: fontName, data: data as! Data, fileExtension: "ttf")
+               try folder.addFile(file: &file)
+               
+//               try fileManager.copyItem(at: customFontURL, to: folder.url!)
+
+               // Register the copied font
+               if let fontDataProvider = CGDataProvider(url: file.url! as CFURL),
+                  let font = CGFont(fontDataProvider) {
+                   var error: Unmanaged<CFError>?
+                   if CTFontManagerRegisterGraphicsFont(font, &error) {
+                       print("Custom font registered successfully.")
+                   } else if let error = error?.takeRetainedValue() {
+                       print("Failed to register custom font: \(error)")
+                   }
+               }
+           } catch {
+               print("Error copying font file: \(error)")
+           }
+       }
+        
+        
+   }
+
+
+}
+
+
