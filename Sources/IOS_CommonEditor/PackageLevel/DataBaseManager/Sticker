+//
+//  MusicManagerVC.swift
+//  MusicManager
+//
+//  Created by HKBeast on 15/11/23.
+//
+
+
+import UIKit
+import MediaPlayer
+
+protocol MusicModelListener {
+    func musicModelUpdate(model: MusicModel)
+}
+
+class MusicManagerVC: UIViewController, MusicLibraryDelegate, ISFavMusicDelegate {
+
+    var dataModel: MusicModel = MusicModel()
+    var customTabBarController = UITabBarController()
+    var delegate: MusicModelListener?
+    @IBOutlet weak var tabbarView: UIView!
+    var recentMusicVC: RecentMusicVC!
+    var musicLibraryVC: MusicLibraryVC!
+    var FavMusicVC: IsFavMusicVC!
+    var importFromiTunesVC: MPMediaPickerController!
+    var sourceType: String!
+
+    init() {
+        super.init(nibName: "MusicManagerVC", bundle: nil)
+    }
+
+    required init?(coder: NSCoder) {
+        fatalError("init(coder:) has not been implemented")
+    }
+
+    func setDelegate(delegate: MusicModelListener) {
+        self.delegate = delegate
+    }
+
+    override func viewDidLoad() {
+        super.viewDidLoad()
+        setupTabBarController()
+        selectSourceforMusic()
+       
+    }
+
+    func setupTabBarController() {
+        customTabBarController.delegate = self
+        customTabBarController.modalPresentationStyle = .overCurrentContext
+        recentMusicVC = RecentMusicVC()
+        let viewModel = MusicLibraryVM(delegate: self)
+        musicLibraryVC = MusicLibraryVC(viewModel: viewModel)
+        let navOne = UINavigationController(rootViewController: musicLibraryVC)
+        let favViewModel = IsFavMusicVM(delegate: self)
+        FavMusicVC = IsFavMusicVC(viewModel: favViewModel)
+        importFromiTunesVC = importFromiTunes()
+
+        // Tab Bar Config
+        FavMusicVC?.tabBarItem = UITabBarItem(title: "Favorites", image: nil, selectedImage: nil)
+        recentMusicVC?.tabBarItem = UITabBarItem(title: "Recent", image: nil, selectedImage: nil)
+        musicLibraryVC?.tabBarItem = UITabBarItem(title: "Library", image: nil, selectedImage: nil)
+        importFromiTunesVC?.tabBarItem = UITabBarItem(title: "iTunes", image: nil, selectedImage: nil)
+       
+
+        // Set the view controllers for the tab bar controller
+        customTabBarController.viewControllers = [FavMusicVC,recentMusicVC, navOne, importFromiTunesVC]
+
+        addChild(customTabBarController)
+        customTabBarController.view.frame = tabbarView.bounds
+        tabbarView.addSubview(customTabBarController.view)
+        customTabBarController.tabBar.backgroundColor = .white
+        customTabBarController.selectedIndex = 2
+    }
+}
+
+extension MusicManagerVC: UITabBarControllerDelegate {
+    func tabBarController(_ tabBarController: UITabBarController, shouldSelect viewController: UIViewController) -> Bool {
+        if tabBarController.selectedIndex == -1 {
+            // Check if the selected view controller is the same as the one being selected
+            if tabBarController.selectedIndex == -1 {
+                // Do any custom logic you want when the tab is clicked again (optional).
+                return true
+            }
+        }
+        // Return true to allow the selection, or false to prevent it.
+        return true
+    }
+
+    func tabBarController(_ customTabBarController: UITabBarController, didSelect viewController: UIViewController) {
+        // viewController.didMove(toParent: self)
+    }
+
+    func selectSourceforMusic() {
+        if sourceType == "Library" {
+            dataModel.sourceType = .Library
+            customTabBarController.selectedIndex = 1
+        } else if sourceType == "iTunes" {
+            dataModel.sourceType = .ITunes
+            customTabBarController.selectedIndex = 2
+        } else 
+        {
+            // Handle other cases as needed
+        }
+    }
+    
+    
+    func importFromiTunes()->MPMediaPickerController {
+        let mediaPicker = MPMediaPickerController(mediaTypes: .music)
+        mediaPicker.delegate = self
+        mediaPicker.allowsPickingMultipleItems = true
+        let safeAreaView = view.safeAreaLayoutGuide
+       return mediaPicker
+    }
+}
+
+// You can implement the specific logic for each tab (Recent, Library, iTunes) in separate view controllers similar to the ImagePicker example.
+enum SourceType:String{
+    case Library = "Library"
+    case ITunes = "iTunes"
+    case Recent = "Recent"
+    case Fav = "Favourite"
+}
+
+
+extension MusicManagerVC : MPMediaPickerControllerDelegate {
+
+    func mediaPicker(_ mediaPicker: MPMediaPickerController, didPickMediaItems mediaItemCollection: MPMediaItemCollection) {
+        dismiss(animated: true, completion: nil)
+
+        // Process the selected media items
+        for mediaItem in mediaItemCollection.items {
+            // Access information about the selected media item (e.g., title, artist, URL)
+            let title = mediaItem.title ?? "Unknown Title"
+            let artist = mediaItem.artist ?? "Unknown Artist"
+            let mediaURL = mediaItem.assetURL
+
+            // Perform actions with the selected media item
+            // ...
+
+            // Notify the delegate or update the data model
+            // delegate?.musicSelected(title: title, artist: artist, mediaURL: mediaURL)
+        }
+    }
+
+    func mediaPickerDidCancel(_ mediaPicker: MPMediaPickerController) {
+        dismiss(animated: true, completion: nil)
+    }
+}
