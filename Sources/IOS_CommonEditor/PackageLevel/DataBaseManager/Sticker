+//
+//  FontPannelVC.swift
+//  FontManager
+//
+//  Created by HKBeast on 06/11/23.
+//
+
+import UIKit
+import MobileCoreServices
+
+class FontPannelVC: UIViewController,UIDocumentPickerDelegate {
+    
+    var fontCV:FontCollectionView?
+    var fontTab:FontTabView?
+    var viewModel : FontPannelVM
+    
+    @IBOutlet weak var fontCVPlaceholder:UIView!
+    @IBOutlet weak var fontTabPlaceholder:UIView!
+    
+    init(viewModel:FontPannelVM){
+        self.viewModel = viewModel
+        super.init(nibName: "FontPannelVC", bundle: nil)
+    }
+    
+    required init?(coder: NSCoder) {
+        fatalError("init(coder:) has not been implemented")
+    }
+    
+    
+    
+    override func viewDidLoad() {
+        super.viewDidLoad()
+
+     createRatioTab()
+//        createRatioCVVC()
+        viewModel.setViewModel()
+    }
+
+    func createRatioCVVC(){
+        if fontCV == nil{
+            fontCV = FontCollectionView(frame:fontCVPlaceholder.frame , viewModel: viewModel.fontCVVM)
+            self.view.addSubview(fontCV!)
+            
+            // Add constraints for ratioCV
+            fontCV?.translatesAutoresizingMaskIntoConstraints = false
+            fontCV?.topAnchor.constraint(equalTo: fontCVPlaceholder.topAnchor).isActive = true
+            fontCV?.leadingAnchor.constraint(equalTo: fontCVPlaceholder.leadingAnchor).isActive = true
+            fontCV?.trailingAnchor.constraint(equalTo: fontCVPlaceholder.trailingAnchor).isActive = true
+            fontCV?.bottomAnchor.constraint(equalTo: fontCVPlaceholder.bottomAnchor).isActive = true
+        }
+    }
+    
+    func createRatioTab(){
+        if fontTab == nil {
+            fontTab = FontTabView(frame: fontTabPlaceholder.frame, viewModel: viewModel.fontTabVM)
+            
+            self.view.addSubview(fontTab!)
+            // Add constraints for ratioTab
+            fontTab?.translatesAutoresizingMaskIntoConstraints = false
+            fontTab?.topAnchor.constraint(equalTo: fontTabPlaceholder.topAnchor).isActive = true
+            fontTab?.leadingAnchor.constraint(equalTo: fontTabPlaceholder.leadingAnchor).isActive = true
+            fontTab?.trailingAnchor.constraint(equalTo: fontTabPlaceholder.trailingAnchor).isActive = true
+            fontTab?.bottomAnchor.constraint(equalTo: fontTabPlaceholder.bottomAnchor).isActive = true
+            
+            }
+    }
+
+    
+    
+    @IBAction func didAddFont(_ sender: UIButton) {
+        
+        importFont()
+        
+    }
+  
+    func importFont() {
+        let documentPicker = UIDocumentPickerViewController(documentTypes: [kUTTypeFont as String], in: .import)
+        documentPicker.delegate = self
+        present(documentPicker, animated: true, completion: nil)
+    }
+
+    
+    
+    func documentPicker(_ controller: UIDocumentPickerViewController, didPickDocumentsAt urls: [URL]) {
+        if let url = urls.first {
+            if let fontData = try? Data(contentsOf: url) {
+                if let fileName = getFontNameFromFontData(fontData: fontData){
+                    var file = File(name: fileName, data: fontData, fileExtension: "")
+                    do{
+                        try AppFileManager.shared.userFont?.addFile(file: &file)
+                        
+                        
+                        let fontModel = FontModel(fontName: fileName, family: "", localPath: "\(fileName).ttf")
+                        
+                        
+                        _  = DBManager.shared.insertFontModel(model: fontModel)
+                        if let fontDataProvider = CGDataProvider(url: file.url! as CFURL),
+                           let font = CGFont(fontDataProvider) {
+                            var error: Unmanaged<CFError>?
+                            if CTFontManagerRegisterGraphicsFont(font, &error) {
+                                print("User font registered successfully.")
+                                
+
+                            } else if let error = error?.takeRetainedValue() {
+                                print("Failed to register custom font: \(error)")
+                            }
+                        }
+                    }
+                    catch{
+                        print("\(fileName) file not added in user folder ")
+                    }
+                    
+                    
+                }
+              }
+           }
+        
+    }
+    
+    func getFontNameFromFontData(fontData: Data) -> String? {
+        if let fontDataProvider = CGDataProvider(data: fontData as CFData),
+           let font = CGFont(fontDataProvider) {
+            
+            if let postScriptName = font.postScriptName as String? {
+                return "\(postScriptName).ttf"
+            }
+        }
+        
+        return nil
+    }
+    
+}
