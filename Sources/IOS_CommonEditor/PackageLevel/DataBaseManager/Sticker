diff --git a/VideoInvitation/Overlay/OverlayCV.swift b/VideoInvitation/Overlay/OverlayCV.swift
new file mode 100644
index 0000000..a79bcb6
--- /dev/null
+++ b/VideoInvitation/Overlay/OverlayCV.swift
@@ -0,0 +1,102 @@
+//
+//  OverlayCV.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 30/11/23.
+//
+
+import Foundation
+import Combine
+import UIKit
+
+class OverlayCV:IOSCollectionView{
+    var viewModel:OverlayCVVM
+    var currentSelectedIndex:Int?
+    private var cancellables: Set<AnyCancellable> = .init()
+    
+    init(frame:CGRect,viewModel:OverlayCVVM){
+        self.viewModel = viewModel
+ 
+        super.init(frame: frame, delegate:nil)
+
+        
+        self.viewModel.$currentOverlay.sink { [self] img in
+            setupCollectionView()
+            if let image = img{
+                if let index = viewModel.overLayArray.firstIndex(where: {$0 == image}){
+                    currentSelectedIndex = index
+                    print("current",currentSelectedIndex)
+                }
+            }
+          
+        }.store(in: &cancellables)
+   
+
+    }
+    
+    required init?(coder: NSCoder) {
+//        super.init(coder: coder)
+        fatalError("init(coder:) has not been implemented")
+//        super.init(frame: coder.accessibilityFrame, delegate: nil)
+     
+    }
+    
+    override func setupCollectionView() {
+        if collectionView == nil {
+            let layout = ReusableCVLayout(numberOfRow: 4, column: 1, and: .horizontal)
+            //            // Configure layout properties
+            //        layout.sectionInset = UIEdgeInsets.init(top: 0, left: 60.0, bottom: 0, right: 60.0)
+            collectionView = UICollectionView(frame: bounds, collectionViewLayout: layout)
+            // JD: autolayout parent
+            collectionView.autoresizingMask = [.flexibleWidth,.flexibleHeight]
+            
+            collectionView.dataSource = self
+            collectionView.delegate = self
+            collectionView.register(UINib(nibName: "OverlayCVCell", bundle: nil), forCellWithReuseIdentifier: "cell")
+            //        collectionView.register(MyCell.nib, forCellWithReuseIdentifier: MyCell.reuseIdentifier)
+            collectionView.backgroundColor = .clear
+            // Register cell classes or nibs
+            
+            // Additional configuration
+            //        layout.scrollDirection = .horizontal
+            self.addSubview(collectionView)
+        }
+        else{
+            collectionView.reloadData()
+        }
+    }
+    
+
+}
+
+extension OverlayCV{
+    override func collectionView(_ collectionView: UICollectionView, numberOfItemsInSection section: Int) -> Int {
+        return viewModel.getCountOfArray()
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, cellForItemAt indexPath: IndexPath) -> UICollectionViewCell{
+        let cell = collectionView.dequeueReusableCell(withReuseIdentifier: "cell", for: indexPath) as! OverlayCVCell
+        if indexPath.row == 0{
+            
+        }
+        else{
+            let imageName = viewModel.getOverlayForInt(index: indexPath.item)
+            cell.config(image: imageName, index: indexPath)
+        }
+        collectionView.selectItem(at: IndexPath(item: currentSelectedIndex ?? 0, section: 0), animated: false, scrollPosition: .centeredVertically)
+        return cell
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, didSelectItemAt indexPath: IndexPath)  {
+        let cell = collectionView.cellForItem(at: indexPath) as! OverlayCVCell
+//       currentSelectedIndex = indexPath
+        viewModel.didChangeOverlay(at: indexPath.item)
+//         cell.selectedCell()
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, didDeselectItemAt indexPath: IndexPath) {
+        let cell = collectionView.cellForItem(at: indexPath) as! OverlayCVCell
+//         cell.deSelectedCell()
+    }
+
+}
