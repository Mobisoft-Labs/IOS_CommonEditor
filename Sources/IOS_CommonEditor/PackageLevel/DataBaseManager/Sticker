+//
+//  AppFileManager.swift
+//  FileManager
+//
+//  Created by HKBeast on 27/10/23.
+//
+
+import Foundation
+
+class exampleAppFleManager{
+    let documentFolder:Folder
+    let tempFolder:Folder
+    var images:Folder? = nil
+    var stickerFolder:Folder? = nil
+    var overLayFolder:Folder? = nil
+    var backgroundFolder:Folder? = nil
+    var audio:Folder? = nil
+    var video:Folder? = nil
+    var dataBase:Folder? = nil
+    
+    init(){
+        documentFolder = RootFolder(type: .Document)
+        tempFolder = RootFolder(type: .Temp)
+    }
+    
+    /*
+     
+     Document Temp
+     
+     DatabaseFolder
+     
+     Assets
+     
+     Images Folder
+     OverlayFolder
+     ThumbnailFolder
+     
+     
+     VideoTempFolder
+     
+     */
+    
+    
+    func configureFolderStructure() {
+      createImageFolder()
+      createDatabaseFolder()
+    }
+    
+    
+    
+    private func createDatabaseFolder(){
+        do {
+            // Create the main "Images" folder in the document directory
+            let databaseFolder = Folder(name: "Database")
+            try documentFolder.addFolder(folder: databaseFolder)
+            print("Images folder created successfully.")
+            dataBase = databaseFolder
+     
+        } catch {
+            print("Error creating folders: \(error.localizedDescription)")
+        }
+    }
+    
+    //MARK: - Image Folder and its SubFolder
+
+    private func createImageFolder() {
+        do {
+            // Create the main "Images" folder in the document directory
+            let imageFolder = Folder(name: "Images")
+            try documentFolder.addFolder(folder: imageFolder)
+            print("Images folder created successfully.")
+            images = imageFolder
+            // Create subfolders within the "Images" folder
+            createStickerFolder(in: imageFolder)
+            createOverlayFolder(in: imageFolder)
+            createBackgroundFolder(in: imageFolder)
+        } catch {
+            print("Error creating folders: \(error.localizedDescription)")
+        }
+    }
+
+    private func createStickerFolder(in parentFolder: Folder) {
+        do {
+            // Create the "Sticker" subfolder within the given parent folder
+            let stickerFolder = Folder(name: "Sticker")
+            try parentFolder.addFolder(folder: stickerFolder)
+                self.stickerFolder = stickerFolder
+            print("Sticker folder created successfully.")
+        } catch {
+            print("Error creating Sticker folder: \(error.localizedDescription)")
+        }
+    }
+
+    private func createOverlayFolder(in parentFolder: Folder) {
+        do {
+            // Create the "Overlay" subfolder within the given parent folder
+            let overlayFolder = Folder(name: "Overlay")
+            try parentFolder.addFolder(folder: overlayFolder)
+            self.overLayFolder = overlayFolder
+            print("Overlay folder created successfully.")
+        } catch {
+            print("Error creating Overlay folder: \(error.localizedDescription)")
+        }
+    }
+
+    private func createBackgroundFolder(in parentFolder: Folder) {
+        do {
+            // Create the "Background" subfolder within the given parent folder
+            let backgroundFolder = Folder(name: "Background")
+            try parentFolder.addFolder(folder: backgroundFolder)
+            self.backgroundFolder = backgroundFolder
+            print("Background folder created successfully.")
+        } catch {
+            print("Error creating Background folder: \(error.localizedDescription)")
+        }
+    }
+   
+    
+    
+//    func getImage(name)
+//    {
+//        
+//        ImageFolder.getFileByName(name)
+//        
+//    }
+}
diff --git a/VideoInvitation/AppFM/File.swift b/VideoInvitation/AppFM/File.swift
new file mode 100644
index 0000000..cb7066d
--- /dev/null
+++ b/VideoInvitation/AppFM/File.swift
@@ -0,0 +1,65 @@
+//
+//  File.swift
+//  FileManager
+//
+//  Created by HKBeast on 26/10/23.
+//
+
+import Foundation
+
+enum FileType{
+    case Audio
+    case Video
+    case DB
+    case Image
+    case None
+}
+
+class File {
+    let name: String
+    var data: Data?
+    var fileExtension: String
+    var fileType: FileType?
+    var url:URL?
+    init(name: String, data: Data, fileExtension: String) {
+        self.name = name
+        self.data = data
+        self.fileExtension = fileExtension
+        self.fileType = determineFileType(from: fileExtension)
+        writeData(data: data)
+    }
+    
+    init(name: String, fileExtension: String) {
+        self.name = name
+        self.fileExtension = fileExtension
+        self.data = nil
+        self.fileType = determineFileType(from: fileExtension)
+        
+    }
+    
+    // Determine the file type based on the file extension
+    private func determineFileType(from fileExtension: String) -> FileType {
+        switch fileExtension {
+        case "nil":
+            return .None
+        case "db":
+            return .DB
+        case "mp3":
+            return .Audio
+        
+        default:
+            return .Image
+        }
+    }
+
+    // Method to read data from the file
+    func readData() -> Data? {
+        return self.data
+    }
+    
+    // Method to write data to the file
+    func writeData(data: Data) {
+        self.data = data
+    }
+}
+
