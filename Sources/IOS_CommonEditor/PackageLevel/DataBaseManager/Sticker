+extension DBManager{
+    
+    func addDefaultPage(templateId:Int)->Int{
+        var model = DBBaseModel()
+        model.parentId = templateId
+        model.modelType = "PAGE"
+        model.templateID = templateId
+        return replaceBaseModelIfNeeded(baseModel: model)
+    }
+  
+    func duplicateModelAndChildren(modelId: Int) {
+        // Ensure that the database is initialized
+        guard dbIsOpen else {
+            print("Database is not initialized.")
+            return
+        }
+
+        database.beginTransaction()
+
+        do {
+            // Step 1: Duplicate the row for MODEL_ID with all the same properties
+            let duplicateRowSQL = """
+               INSERT INTO MODEL (
+                   MODEL_TYPE, DATA_ID, POS_X, POS_Y, WIDTH, HEIGHT,
+                   PREV_AVAILABLE_WIDTH, PREV_AVAILABLE_HEIGHT, ROTATION,
+                   MODEL_OPACITY, MODEL_FLIP_HORIZONTAL, MODEL_FLIP_VERTICAL, LOCK_STATUS,
+                   ORDER_IN_PARENT, PARENT_ID, BG_BLUR_PROGRESS, OVERLAY_DATA_ID,
+                   OVERLAY_OPACITY, START_TIME, DURATION, SOFT_DELETE,TEMPLATE_ID
+               )
+               SELECT
+                   MODEL_TYPE, DATA_ID, POS_X, POS_Y, WIDTH, HEIGHT,
+                   PREV_AVAILABLE_WIDTH, PREV_AVAILABLE_HEIGHT, ROTATION,
+                   MODEL_OPACITY, MODEL_FLIP_HORIZONTAL, MODEL_FLIP_VERTICAL, LOCK_STATUS,
+                   ORDER_IN_PARENT, PARENT_ID, BG_BLUR_PROGRESS, OVERLAY_DATA_ID,
+                   OVERLAY_OPACITY, START_TIME, DURATION, SOFT_DELETE,TEMPLATE_ID
+               FROM MODEL
+               WHERE MODEL_ID = \(modelId)
+
+            """
+        
+
+            // Step 2: Get the entry's MODEL_ID with name NewParentModel_ID variable
+            let newParentModelID = insertNewEntry(query: duplicateRowSQL)
+
+            // Step 3: Get all rows in that table whose PARENT_ID is the same as the Parameter MODEL_ID
+            let getChildRowsSQL = "SELECT * FROM MODEL WHERE PARENT_ID = \(modelId) AND MODEL_TYPE != 'PAGE'"
+            let childRows = try runQuery(getChildRowsSQL, values: nil)
+
+            // Step 4: Duplicate all rows with New MODEL_ID and PARENT_ID = newParenModel_ID
+            while childRows.next() {
+                let childModelID = try childRows.int(forColumn: "MODEL_ID")
+                let duplicateChildRowSQL = """
+    INSERT INTO MODEL
+    SELECT null,
+           MODEL_TYPE,
+           DATA_ID,
+           POS_X,
+           POS_Y,
+           WIDTH,
+           HEIGHT,
+           PREV_AVAILABLE_WIDTH,
+           PREV_AVAILABLE_HEIGHT,
+           ROTATION,
+           MODEL_OPACITY,
+           MODEL_FLIP_HORIZONTAL,
+           MODEL_FLIP_VERTICAL,
+           LOCK_STATUS,
+           ORDER_IN_PARENT,
+           \(newParentModelID) AS PARENT_ID,
+           BG_BLUR_PROGRESS,
+           OVERLAY_DATA_ID,
+           OVERLAY_OPACITY,
+           START_TIME,
+           DURATION,
+           0,
+           TEMPLATE_ID
+    FROM MODEL
+    WHERE MODEL_ID = \(childModelID)
+"""
+                try insertNewEntry(query: duplicateChildRowSQL)
+
+                // Step 5: Recursive check if all rows have MODEL_TYPE = "PARENT"
+                let isParentType = try childRows.string(forColumn: "MODEL_TYPE") == "PARENT"
+                if isParentType {
+                    // Recursive call for duplicating children
+                    try duplicateChildren(modelId: Int(childModelID), newParentId: Int(newParentModelID))
+                }
+            }
+
+            // Commit the transaction
+            database.commit()
+        } catch {
+            print("Error: \(error)")
+            // Rollback the transaction in case of an error
+            database.rollback()
+        }
+    }
+    private func duplicateChildren(modelId: Int, newParentId: Int) throws {
+        // Step 1: Get all rows in that table whose PARENT_ID is the same as the Parameter MODEL_ID
+        let getChildRowsSQL = "SELECT * FROM MODEL WHERE PARENT_ID = \(modelId) AND MODEL_TYPE != 'PAGE'"
+        let childRows = try runQuery(getChildRowsSQL, values: nil)
+
+        // Step 2: Duplicate all rows with New MODEL_ID and PARENT_ID = newParentModel_ID
+        while childRows.next() {
+            let childModelID = try childRows.int(forColumn: "MODEL_ID")
+            let duplicateChildRowSQL = """
+    INSERT INTO MODEL
+    SELECT null,
+           MODEL_TYPE,
+           DATA_ID,
+           POS_X,
+           POS_Y,
+           WIDTH,
+           HEIGHT,
+           PREV_AVAILABLE_WIDTH,
+           PREV_AVAILABLE_HEIGHT,
+           ROTATION,
+           MODEL_OPACITY,
+           MODEL_FLIP_HORIZONTAL,
+           MODEL_FLIP_VERTICAL,
+           LOCK_STATUS,
+           ORDER_IN_PARENT,
+           \(newParentId) AS PARENT_ID,
+           BG_BLUR_PROGRESS,
+           OVERLAY_DATA_ID,
+           OVERLAY_OPACITY,
+           START_TIME,
+           DURATION,
+           0,
+           TEMPLATE_ID
+    FROM MODEL
+    WHERE MODEL_ID = \(childModelID)
+"""
+                try insertNewEntry(query: duplicateChildRowSQL)
+
+
+            // Step 3: Recursive call for duplicating children
+            try duplicateChildren(modelId: Int(childModelID), newParentId: newParentId)
+        }
+    }
+     // ... (existing code)
+ 
+}
