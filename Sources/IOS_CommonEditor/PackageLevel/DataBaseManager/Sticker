diff --git a/VideoInvitation/Music_manager/MusicModel.swift b/VideoInvitation/Music_manager/MusicModel.swift
new file mode 100644
index 0000000..b36457c
--- /dev/null
+++ b/VideoInvitation/Music_manager/MusicModel.swift
@@ -0,0 +1,32 @@
+//
+//  MusicModel.swift
+//  MusicManager
+//
+//  Created by HKBeast on 15/11/23.
+//
+
+import Foundation
+import MediaPlayer
+
+struct MusicModel {
+    var musicID: Int = 0
+    var musicName: String = ""
+    var localPath: String = ""
+    var displayName: String = ""
+    var fileExtension: String = ""
+    var serverPath: String = ""
+    var thumbImageServerPath: String = ""
+    var thumbImageLocalPath: String = ""
+    var isFav: Bool = false
+    var sourceType:SourceType = .Recent
+    
+    
+}
+
+extension MusicModel{
+    func getAVAsset()->AVURLAsset?{
+        let path = Bundle.main.path(forResource: "\(localPath)", ofType:"mp3")
+        let soundAsset = AVURLAsset(url: URL.init(fileURLWithPath: path ?? ""))
+        return soundAsset
+    }
+}
