+//
+
+import UIKit
+
+class StickerPcikerVC: UIViewController {
+
+    
+    var stickerCV:StickerCV?
+    var stickerTab:StickerTab?
+    var viewModel : StickerPickerVM
+    
+    @IBOutlet weak var stickerCVPlaceholder: UIView!
+
+    @IBOutlet weak var stickerTabPlaceholder: UIView!
+  
+    
+    init(viewModel:StickerPickerVM){
+        self.viewModel = viewModel
+        super.init(nibName: "FontPannelVC", bundle: nil)
+    }
+    
+    required init?(coder: NSCoder) {
+        fatalError("init(coder:) has not been implemented")
+    }
+    
+    
+    
+    override func viewDidLoad() {
+        super.viewDidLoad()
+
+     createRatioTab()
+        createRatioCVVC()
+//        viewModel.setViewModel()
+    }
+
+    func createRatioCVVC(){
+        if stickerCV == nil{
+            stickerCV = StickerCV(frame:stickerCVPlaceholder.frame , viewModel: viewModel.stickerCVVM)
+            self.view.addSubview(stickerCV!)
+            
+            // Add constraints for ratioCV
+            stickerCV?.translatesAutoresizingMaskIntoConstraints = false
+            stickerCV?.topAnchor.constraint(equalTo: stickerCVPlaceholder.topAnchor).isActive = true
+            stickerCV?.leadingAnchor.constraint(equalTo: stickerCVPlaceholder.leadingAnchor).isActive = true
+            stickerCV?.trailingAnchor.constraint(equalTo: stickerCVPlaceholder.trailingAnchor).isActive = true
+            stickerCV?.bottomAnchor.constraint(equalTo: stickerCVPlaceholder.bottomAnchor).isActive = true
+        }
+    }
+    
+    func createRatioTab(){
+//        if fontTab == nil {
+//            fontTab = FontTabView(frame: fontTabPlaceholder.frame, viewModel: viewModel.fontTabVM)
+//            
+//            self.view.addSubview(fontTab!)
+//            // Add constraints for ratioTab
+//            fontTab?.translatesAutoresizingMaskIntoConstraints = false
+//            fontTab?.topAnchor.constraint(equalTo: fontTabPlaceholder.topAnchor).isActive = true
