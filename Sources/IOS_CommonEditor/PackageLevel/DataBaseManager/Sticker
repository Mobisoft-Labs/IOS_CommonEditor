+    
+   static func getModel(array:[String],category:String)->[StickerAsset]{
+        var stickerMasterInfo1 = [StickerAsset]()
+        for ar in array{
+            var stickerMasterInfo = StickerAsset()
+            stickerMasterInfo.category = category // Assuming TITLES[i] contains the category name
+            stickerMasterInfo.imageName = ar
+            stickerMasterInfo1.append(stickerMasterInfo)
+        }
+        return stickerMasterInfo1
+    }
