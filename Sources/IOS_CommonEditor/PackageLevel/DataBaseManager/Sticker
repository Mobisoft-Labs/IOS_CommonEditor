diff --git a/VideoInvitation/Overlay/OverlayCVVM.swift b/VideoInvitation/Overlay/OverlayCVVM.swift
new file mode 100644
index 0000000..819a018
--- /dev/null
+++ b/VideoInvitation/Overlay/OverlayCVVM.swift
@@ -0,0 +1,47 @@
+//
+//  OverlayCVVM.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 30/11/23.
+//
+
+import Foundation
+
+protocol overlayDelegate:AnyObject{
+    func onChangeOverlay(overlay:String)
+}
+
+class OverlayCVVM{
+    
+    weak var overlayDelegate:overlayDelegate?
+    @Published var currentOverlay:String?
+    
+    var overLayArray = ["none","o1", "o2",
+                        "o3", "o4", "o5", "o6",
+                        "o7", "o8", "o9", "o10",
+                        "o11", "o12", "o13", "o14",
+                        "o15", "o16", "o17", "o18", "o19", "o20", "o21",
+                        "o22", "o23", "o24", "o25", "o26", "o27", "o28", "o29",
+                        "o30", "o31"]
+    
+    
+    init(delegate:overlayDelegate){
+        self.overlayDelegate = delegate
+    }
+    
+    
+    func getCountOfArray()->Int{
+        return overLayArray.count
+    }
+    
+    func getOverlayForInt(index:Int)->String{
+        return overLayArray[index]
+    }
+    
+    func didChangeOverlay(at:Int){
+        overlayDelegate?.onChangeOverlay(overlay: overLayArray[at])
+    }
+    func setModel(overlay:String){
+        currentOverlay = overlay
+    }
+}
diff --git a/VideoInvitation/Overlay/OverlayPannelVC.swift b/VideoInvitation/Overlay/OverlayPannelVC.swift
new file mode 100644
index 0000000..f43c85c
--- /dev/null
+++ b/VideoInvitation/Overlay/OverlayPannelVC.swift
@@ -0,0 +1,66 @@
+//
+//  OverlayPannelVC.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 30/11/23.
+//
+
+import UIKit
+import Combine
+
+class OverlayPannelVC: UIViewController {
+
+    
+    @IBOutlet weak var overlayCVPlaceholder:UIView!
+    @IBOutlet weak var overlayOpacitySlider:UISlider!
+    private var cancellables: Set<AnyCancellable> = .init()
+    var viewModel = OverlayPannelVM()
+    
+    
+    override func viewDidLoad() {
+        super.viewDidLoad()
+
+        let vm = viewModel.overlayCVVM
+        let cv = OverlayCV(frame: overlayCVPlaceholder.frame, viewModel: vm)
+        self.view.addSubview(cv)
+      
+        viewModel.$currentOverlayOpacity.sink { [self] value in
+            overlayOpacitySlider.value = Float(value)
+            vm.setModel(overlay: viewModel.currentOverlay ?? "none")
+        }.store(in: &cancellables)
+        
+    }
+
+    @IBAction func overlayOpacitySliderChanged(_ sender: UISlider, forEvent event: UIEvent) {
+        if let touchEvent = event.allTouches?.first {
+               switch touchEvent.phase {
+               case .began:
+                   // Begin state: The user just started interacting with the slider
+                   print("Slider Touch Down - Begin State")
+                 
+                   // Additional code for begin state if needed
+
+               case .moved:
+                   // Current state: The user is actively sliding the slider
+                   print("Slider Value Changed - Current State")
+                   // Additional code for current state if needed
+
+                   // You can update your viewModel with the current value here if needed
+                 
+
+               case .ended:
+                   // End state: The user released their finger, ending the interaction
+                   viewModel.didChangeOpacity(value: sender.value)
+                   print("Slider Touch Up Inside - End State")
+                   // Additional code for end state if needed
+
+               default:
+                   break
+               }
+           }
+        
+    }
+    
+    
+
+}
