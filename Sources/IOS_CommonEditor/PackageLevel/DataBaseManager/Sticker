diff --git a/VideoInvitation/Music_manager/Fav Music/IsFavMusicVC.swift b/VideoInvitation/Music_manager/Fav Music/IsFavMusicVC.swift
new file mode 100644
index 0000000..167970a
--- /dev/null
+++ b/VideoInvitation/Music_manager/Fav Music/IsFavMusicVC.swift	
@@ -0,0 +1,79 @@
+//
+//  IsFavMusicVC.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 21/11/23.
+//
+
+import UIKit
+import Combine
+
+class IsFavMusicVC: UIViewController  , UITableViewDelegate, UITableViewDataSource {
+        // Table View
+         var tableView: UITableView!
+        var viewModel:IsFavMusicVM
+    private var cancellables: Set<AnyCancellable> = .init()
+        init(viewModel:IsFavMusicVM){
+            self.viewModel = viewModel
+            super.init(nibName: "IsFavMusicVC", bundle: nil)
+          
+        }
+        
+        required init?(coder: NSCoder) {
+            fatalError("init(coder:) has not been implemented")
+        }
+        
+        override func viewDidLoad() {
+            super.viewDidLoad()
+            
+           activateSubscriber()
+            viewModel.setModel()
+        }
+
+        func setupTableView(){
+            // Set up the table view programmatically
+            tableView = UITableView(frame: view.bounds, style: .plain)
+            tableView.delegate = self
+            tableView.dataSource = self
+            tableView.register(UITableViewCell.self, forCellReuseIdentifier: "cell")
+            view.addSubview(tableView)
+        }
+
+    func activateSubscriber(){
+        viewModel.$isMusicArrayFetched.sink { [weak self] bool in
+            if bool{
+                self?.setupTableView()
+            }
+        }.store(in: &cancellables)
+    }
+    
+    
+        // MARK: - Table View DataSource
+
+        func tableView(_ tableView: UITableView, numberOfRowsInSection section: Int) -> Int {
+            return viewModel.getCountOfMusicArray()
+        }
+
+        func tableView(_ tableView: UITableView, cellForRowAt indexPath: IndexPath) -> UITableViewCell {
+            let cell = tableView.dequeueReusableCell(withIdentifier: "cell", for: indexPath)
+    //        let item = viewModel.items[indexPath.row]
+             
+            cell.textLabel?.text = viewModel.getMusicModelFor(index: indexPath.row).displayName
+    //        cell.accessoryType = item.isFavorite ? .checkmark : .none
+
+            return cell
+        }
+
+        // MARK: - Table View Delegate
+
+        func tableView(_ tableView: UITableView, didSelectRowAt indexPath: IndexPath) {
+            tableView.deselectRow(at: indexPath, animated: true)
+
+            // Toggle the isFavorite property when a row is selected
+    //        viewModel.items[indexPath.row].isFavorite.toggle()
+            tableView.reloadRows(at: [indexPath], with: .automatic)
+        }
+
+
+
+}
