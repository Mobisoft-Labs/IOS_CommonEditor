+//
+//  RatioVM.swift
+//  RatioManager
+//
+//  Created by HKBeast on 30/10/23.
+//
+
+import Foundation
+
+class FontCVVM{
+    
+  
+    private var currentModelID = -1
+    private var currentModelArrayCategory = " "
+    
+    @Published var onSelectModelIndex:IndexPath = IndexPath(item: -1, section: 0)
+    @Published var onFontModelArrayLoaded : Bool = false
+    var fontArray = [FontModel]()
+    
+    weak var ratioCVDelegate:FontCollectionViewDelegate?
+
+    
+    init(ratioCVDelegate:FontCollectionViewDelegate){
+        self.ratioCVDelegate = ratioCVDelegate
+    }
+    
+    func getFontModelArrayCount()->Int{
+        return fontArray.count
+    }
+    
+    func getFontModelFor(index:Int)->FontModel{
+        return fontArray[index]
+    }
+    
+    func setFontCV(fontArray:[FontModel]){
+        self.fontArray = fontArray
+        onFontModelArrayLoaded = true
+    }
+    
+}
diff --git a/VideoInvitation/FontManager/FontPannel/RatioCollectionView/FontCollectionView.swift b/VideoInvitation/FontManager/FontPannel/RatioCollectionView/FontCollectionView.swift
new file mode 100644
index 0000000..fa0be5b
--- /dev/null
+++ b/VideoInvitation/FontManager/FontPannel/RatioCollectionView/FontCollectionView.swift
@@ -0,0 +1,100 @@
+//
+//  BaseCV.swift
+//  RatioManager
+//
+//  Created by HKBeast on 30/10/23.
+//
+
+import Foundation
+import UIKit
+import Combine
+
+protocol FontCollectionViewDelegate:AnyObject{
+//    func didRatioModelSelected(model:DBRatioTableModel)
+}
+
+
+class FontCollectionView:IOSCollectionView{
+   
+    var viewModel:FontCVVM
+    var currentSelectedIndex:IndexPath?
+    private var cancellables: Set<AnyCancellable> = .init()
+    
+    init(frame:CGRect,viewModel:FontCVVM){
+        self.viewModel = viewModel
+ 
+        super.init(frame: frame, delegate:nil)
+
+        activateBinder()
+
+    }
+    
+    required init?(coder: NSCoder) {
+//        super.init(coder: coder)
+        fatalError("init(coder:) has not been implemented")
+//        super.init(frame: coder.accessibilityFrame, delegate: nil)
+     
+    }
+    
+    override func setupCollectionView() {
+        if collectionView == nil {
+            let layout = ReusableCVLayout(numberOfRow: 6, column: 2, and: .horizontal)
+            //            // Configure layout properties
+            //        layout.sectionInset = UIEdgeInsets.init(top: 0, left: 60.0, bottom: 0, right: 60.0)
+            collectionView = UICollectionView(frame: bounds, collectionViewLayout: layout)
+            // JD: autolayout parent
+            collectionView.autoresizingMask = [.flexibleWidth,.flexibleHeight]
+            
+            collectionView.dataSource = self
+            collectionView.delegate = self
+            collectionView.register(UINib(nibName: "FontCVCell", bundle: nil), forCellWithReuseIdentifier: "cell")
+            //        collectionView.register(MyCell.nib, forCellWithReuseIdentifier: MyCell.reuseIdentifier)
+            collectionView.backgroundColor = .clear
+            // Register cell classes or nibs
+            
+            // Additional configuration
+            //        layout.scrollDirection = .horizontal
+            self.addSubview(collectionView)
+        }
+        else{
+            collectionView.reloadData()
+        }
+    }
+    
+    func activateBinder(){
+   
+        viewModel.$onFontModelArrayLoaded.dropFirst().sink { bool in
+            if bool{
+                self.setupCollectionView()
+                
+            }
+        }.store(in: &cancellables)
+    }
+}
+
+extension FontCollectionView{
+    override func collectionView(_ collectionView: UICollectionView, numberOfItemsInSection section: Int) -> Int {
+        return viewModel.getFontModelArrayCount()
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, cellForItemAt indexPath: IndexPath) -> UICollectionViewCell{
+        let cell = collectionView.dequeueReusableCell(withReuseIdentifier: "cell", for: indexPath) as! FontCVCell
+        let model = viewModel.getFontModelFor(index: indexPath.item)
+        cell.config(model: model, indexPath: indexPath)
+        return cell
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, didSelectItemAt indexPath: IndexPath)  {
+        let cell = collectionView.cellForItem(at: indexPath) as! FontCVCell
+//       currentSelectedIndex = indexPath
+     
+        cell.selectedCell()
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, didDeselectItemAt indexPath: IndexPath) {
+        let cell = collectionView.cellForItem(at: indexPath) as! FontCVCell
+        cell.deSelectedCell()
+    }
+
+
+}
