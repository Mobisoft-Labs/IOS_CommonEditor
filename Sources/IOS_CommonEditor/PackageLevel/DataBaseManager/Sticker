diff --git a/VideoInvitation/PageController/PageControllerCV.swift b/VideoInvitation/PageController/PageControllerCV.swift
new file mode 100644
index 0000000..b02bce4
--- /dev/null
+++ b/VideoInvitation/PageController/PageControllerCV.swift
@@ -0,0 +1,94 @@
+//
+//  PageControllerCV.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 24/11/23.
+//
+
+import Foundation
+import UIKit
+import Combine
+
+class PageControllerCV:IOSCollectionView{
+    
+     var viewModel:PageControllerCVVM
+     var currentSelectedIndex:IndexPath?
+     private var cancellables: Set<AnyCancellable> = .init()
+     
+     init(frame:CGRect,viewModel:PageControllerCVVM){
+         self.viewModel = viewModel
+  
+         super.init(frame: frame, delegate:nil)
+
+         activateBinder()
+
+     }
+     
+     required init?(coder: NSCoder) {
+ //        super.init(coder: coder)
+         fatalError("init(coder:) has not been implemented")
+ //        super.init(frame: coder.accessibilityFrame, delegate: nil)
+      
+     }
+     
+     override func setupCollectionView() {
+         if collectionView == nil {
+             let layout = ReusableCVLayout(numberOfRow: 4, column: 1, and: .horizontal)
+             //            // Configure layout properties
+             //        layout.sectionInset = UIEdgeInsets.init(top: 0, left: 60.0, bottom: 0, right: 60.0)
+             collectionView = UICollectionView(frame: bounds, collectionViewLayout: layout)
+             // JD: autolayout parent
+             collectionView.autoresizingMask = [.flexibleWidth,.flexibleHeight]
+             
+             collectionView.dataSource = self
+             collectionView.delegate = self
+             collectionView.register(UINib(nibName: "PageCVCell", bundle: nil), forCellWithReuseIdentifier: "cell")
+             //        collectionView.register(MyCell.nib, forCellWithReuseIdentifier: MyCell.reuseIdentifier)
+             collectionView.backgroundColor = .clear
+             // Register cell classes or nibs
+             
+             // Additional configuration
+             //        layout.scrollDirection = .horizontal
+             self.addSubview(collectionView)
+         }
+     }
+     
+     func activateBinder(){
+         
+         
+         viewModel.$isPageFetched.dropFirst().sink { [self] bool in
+                 setupCollectionView()
+             collectionView.reloadData()
+             
+             
+         }.store(in: &cancellables)
+         
+    
+     }
+ }
+
+ extension PageControllerCV{
+     override func collectionView(_ collectionView: UICollectionView, numberOfItemsInSection section: Int) -> Int {
+         return viewModel.getPageCount()
+     }
+     
+     override func collectionView(_ collectionView: UICollectionView, cellForItemAt indexPath: IndexPath) -> UICollectionViewCell{
+         let cell = collectionView.dequeueReusableCell(withReuseIdentifier: "cell", for: indexPath) as! PageCVCell
+         let model = viewModel.getPage(at: indexPath.item)
+         cell.config(model: model, indexPath: indexPath)
+         return cell
+     }
+     
+     override func collectionView(_ collectionView: UICollectionView, didSelectItemAt indexPath: IndexPath)  {
+         let cell = collectionView.cellForItem(at: indexPath) as! PageCVCell
+ //       currentSelectedIndex = indexPath
+//         viewModel.
+//         cell.selectedCell()
+     }
+     
+     override func collectionView(_ collectionView: UICollectionView, didDeselectItemAt indexPath: IndexPath) {
+         let cell = collectionView.cellForItem(at: indexPath) as! PageCVCell
+//         cell.deSelectedCell()
+     }
+
+}
diff --git a/VideoInvitation/PageController/PageControllerCVVM.swift b/VideoInvitation/PageController/PageControllerCVVM.swift
new file mode 100644
index 0000000..b0e8ff7
--- /dev/null
+++ b/VideoInvitation/PageController/PageControllerCVVM.swift
@@ -0,0 +1,69 @@
+//
+//  PageControllerCVVM.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 24/11/23.
+//
+
+import Foundation
+
+protocol PageControllerCVVMDelegate:AnyObject{
+    
+}
+
+
+class PageControllerCVVM{
+    
+    weak var delegate:PageControllerCVVMDelegate?
+   @Published var isPageFetched = true
+    var pageArray = [DBBaseModel]()
+    var currentModelID = 0
+    var currentTemplate = 0
+    init(delegate:PageControllerCVVMDelegate){
+        self.delegate = delegate
+        
+    }
+    
+    func getPageArray(id:Int){
+       pageArray = DBManager.shared.getActivePagesList(templateId: id)
+        currentTemplate = id
+        currentModelID = pageArray[0].modelId
+        isPageFetched = true
+    }
+    
+    func updatePageArray(){
+        pageArray = DBManager.shared.getActivePagesList(templateId: currentTemplate)
+         currentModelID = pageArray[0].modelId
+         isPageFetched = true
+    }
+    
+    func deletePageFor(id:Int){
+        let delete = DBManager.shared.deletePage(for: currentModelID)
+        updatePageArray()
+       
+    }
+    
+    func addPage(){
+        DBManager.shared.addDefaultPage(templateId: currentTemplate)
+        updatePageArray()
+  
+         
+    }
+    
+    func addDuplicatePage(for id:Int){
+        DBManager.shared.duplicateModelAndChildren(modelId: currentModelID)
+        updatePageArray()
+    }
+    
+    func setModel(templateId:Int){
+        getPageArray(id: templateId)
+    }
+    
+    func getPageCount()->Int{
+        return pageArray.count
+    }
+    
+    func getPage(at index:Int)->DBBaseModel{
+        return pageArray[index]
+    }
+}
