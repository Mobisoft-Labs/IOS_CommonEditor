+import Foundation
+
+class StickerAsset{
+    
+    var id: Int = -1
+    var category: String = ""
+    let displaySequence: Int = 0
+    let resourceType: String = ""
+    var imageName: String = ""
+    let imageLocalPath: String = ""
+    let imageServerPath: String = ""
+    let imageRelativePath: String = ""
+    let imageFileExtension: String = ""
+    let imageThumbServerPath: String = ""
+    let imageThumbRelativePath: String = ""
+    let imageThumbFileExtension: String = ""
+    let imageWidth: Int = 0
+    let imageHeight: Int = 0
+    let thumbWidth: Int = 0
+    let thumbHeight: Int = 0
+    let tags: String = ""
+    let isFree: Bool = false
+    let isLocal: Bool = false
+    let isMarked: Bool = false
+    let isEncrypted: Bool = false
+    let futureField1: String = ""
+    let futureField2: String = ""
+    let futureField3: String = ""
+    let futureField4: String = ""
+    
+}
