+import UIKit
+
+class FontTabCVCell: UICollectionViewCell {
+
+    @IBOutlet weak var ratioCategoryName:UILabel!
+    var category:String = " "
+    override func awakeFromNib() {
+        super.awakeFromNib()
+        // Initialization code
+       
+        self.layer.borderWidth = 1.0
+        if self.isSelected{
+            selectedCell()
+        }
+        else{
+            deSelectedCell()
+        }
+        
+    }
+
+    func configCell(categoryName:String,indexPath:IndexPath , isselect:Bool ){
+        category = categoryName
+        ratioCategoryName.text = category
+        if isselect{
+            selectedCell()
+        }
+        else{
+            deSelectedCell()
+        }
+        
+    }
+//    override var isSelected: Bool {
+//        didSet {
+//            isSelected ? selectedCell() : deSelectedCell()
+//        }
+//    }
+//    
+    
+    func selectedCell(){
+        self.isSelected = true
+//        self.layer.borderColor = .blue
+    }
+    
+    func deSelectedCell(){
+        self.isSelected = false
+//        self.layer.borderColor = .white
+    }
+    
+}
