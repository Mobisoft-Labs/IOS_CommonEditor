+//
+//  RatioTabBarView.swift
+//  RatioManager
+//
+//  Created by HKBeast on 30/10/23.
+//
+
+import Foundation
+import UIKit
+import Combine
+
+protocol FontTAbDelegate:AnyObject{
+    func didFontCategorySelected(categoryName:String)
+}
+class FontTabView:BaseTabBarView{
+ 
+    var viewModel : FontTabVM
+    
+    var selectedIndexPath:IndexPath?
+    private var cancellables: Set<AnyCancellable> = .init()
+    
+    init(frame:CGRect,viewModel:FontTabVM){
+        self.viewModel = viewModel
+        super.init(frame: frame, delegate:nil)
+        activateBinder()
+   
+    }
+    
+    required init?(coder: NSCoder) {
+//        super.init(coder: coder)
+        fatalError("init(coder:) has not been implemented")
+
+    }
+    
+    override func setupCollectionView() {
+        if collectionView == nil {
+            let layout = ReusableCVLayout(numberOfRow: 4, column: 1, and: .horizontal)
+            //            // Configure layout properties
+            //        layout.sectionInset = UIEdgeInsets.init(top: 0, left: 60.0, bottom: 0, right: 60.0)
+            collectionView = UICollectionView(frame: bounds, collectionViewLayout: layout)
+            // JD: autolayout parent
+            collectionView.autoresizingMask = [.flexibleWidth,.flexibleHeight]
+            
+            collectionView.dataSource = self
+            collectionView.delegate = self
+            collectionView.register(UINib(nibName: "FontTabCVCell", bundle: nil), forCellWithReuseIdentifier: "cell")
+            //        collectionView.register(MyCell.nib, forCellWithReuseIdentifier: MyCell.reuseIdentifier)
+            collectionView.backgroundColor = .clear
+            // Register cell classes or nibs
+            
+            // Additional configuration
+            //        layout.scrollDirection = .horizontal
+            self.addSubview(collectionView)
+        }
+    }
+    
+    func activateBinder(){
+        viewModel.$onCategoryListLoaded.dropFirst().sink { bool in
+            if bool{
+                self.setupCollectionView()
+            }
+        }.store(in: &cancellables)
+    }
+    
+}
+extension FontTabView{
+    
+    override func collectionView(_ collectionView: UICollectionView, numberOfItemsInSection section: Int) -> Int {
+        viewModel.getFontCategoriesCount()
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, cellForItemAt indexPath: IndexPath) -> UICollectionViewCell {
+        let cell = collectionView.dequeueReusableCell(withReuseIdentifier: "cell", for: indexPath) as! FontTabCVCell
+        let category = viewModel.getFontCategories()
+        cell.configCell(categoryName: category[indexPath.item], indexPath: indexPath,isselect: indexPath == self.selectedIndexPath)
+
+        return cell
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, didSelectItemAt indexPath: IndexPath) {
+        let cell = collectionView.cellForItem(at: indexPath) as! FontTabCVCell
+//        selectedIndexPath = indexPath
+    
+        cell.selectedCell()
+        viewModel.onSelectCategory(index: indexPath.item)
+    }
+    
+    override func collectionView(_ collectionView: UICollectionView, didDeselectItemAt indexPath: IndexPath) {
+         let cell = collectionView.cellForItem(at: indexPath) as! FontTabCVCell
+         cell.deSelectedCell()
+    }
+    
+    
+    
+//    
+//    func flowLayout(frame: CGRect) -> UICollectionViewFlowLayout {
+//        return ReusableCVLayout(numberOfRow: 4, column: 1, and: .horizontal)
+//    }
+}
