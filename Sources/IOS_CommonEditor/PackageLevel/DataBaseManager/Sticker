+//
+//  PageControllerVC.swift
+//  VideoInvitation
+//
+//  Created by HKBeast on 24/11/23.
+//
+
+import UIKit
+
+class PageControllerVC: UIViewController {
+
+    @IBOutlet weak var pageControllerCollectionViewPlaceholder:UIView!
+    @IBOutlet weak var addPageButton:UIButton!
+    @IBOutlet weak var duplicatePageButton:UIButton!
+    @IBOutlet weak var deletePageButton:UIButton!
+    var viewModel:PageControllerVM
+    var pageCV:PageControllerCV?
+    
+    init(viewModel:PageControllerVM){
+        self.viewModel = viewModel
+        super.init(nibName: "PageControllerVC", bundle: nil)
+    }
+    
+    required init?(coder: NSCoder) {
+        fatalError("init(coder:) has not been implemented")
+    }
+    
+    override func viewDidLoad() {
+        super.viewDidLoad()
+
+        pageCV = PageControllerCV(frame: pageControllerCollectionViewPlaceholder.frame, viewModel: viewModel.pageControllerCVVM)
+        self.pageControllerCollectionViewPlaceholder.addSubview(pageCV!)
+        viewModel.pageControllerCVVM.setModel(templateId: 240)
+        
+    }
+
+    @IBAction func didAddPageClicked(_ sender:UIButton){
+        viewModel.onClickAddPage()
+    }
+    @IBAction func didDuplicatePageClicked(_ sender:UIButton){
+        viewModel.onClickDuplicatePage(for: 240)
+    }
+    @IBAction func didDeletePageClicked(_ sender:UIButton){
+        
+    }
+
+}
